{ 
    "version": 2, 
    "builds": [
    { "src": "/dist/index.html", "use": "@vercel/static" },
    { "src": "/api/index.js", "use": "@vercel/node" }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" },
        { "source": "/api/(.*)", "destination": "/api" }
    ]
}